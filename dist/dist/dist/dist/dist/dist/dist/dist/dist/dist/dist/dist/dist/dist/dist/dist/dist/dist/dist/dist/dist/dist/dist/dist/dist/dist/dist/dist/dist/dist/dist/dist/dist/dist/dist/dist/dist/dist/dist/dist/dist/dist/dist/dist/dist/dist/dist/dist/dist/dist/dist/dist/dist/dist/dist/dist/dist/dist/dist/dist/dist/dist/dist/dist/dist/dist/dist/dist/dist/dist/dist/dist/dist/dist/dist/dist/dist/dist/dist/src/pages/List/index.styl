#page-nav
  text-align: center
  margin-top: 30px
  .extend
    color: #4d4d4d
    margin: 0 27px
    opacity: 1
  .page-number
    width: 20px
    height: 25px
    background: #4d4d4d
    display: inline-block
    color: #fff
    line-height: 25px
    font-size: 12px
    margin: 0 5px 30px
    border-radius: 2px
  .current
    background: #88acdb
    cursor: default